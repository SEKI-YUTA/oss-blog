{
  "name": "tech-museum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:metadata": "pnpm gen:contributors && pnpm gen:search && pnpm gen:insights",
    "start": "next start",
    "lint": "eslint . --max-warnings=0 --config eslint.config.mjs",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check --cache .",
    "format:write": "prettier --write --cache .",
    "gen:article": "plop article",
    "gen:contributors": "tsx scripts/contributors",
    "gen:search": "tsx scripts/search",
    "gen:insights": "tsx scripts/report-insights.ts",
    "migrate": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@newrelic/next": "^0.10.0",
    "@types/newrelic": "^9.14.7",
    "@types/next": "^9.0.0",
    "@yamada-ui/react": "^1.7.4",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "figlet": "^1.8.0",
    "glob": "^11.0.1",
    "gray-matter": "^4.0.3",
    "match-sorter": "^6.3.4",
    "mermaid": "^11.4.1",
    "newrelic": "^12.13.0",
    "next": "14.1.1",
    "next-auth": "5.0.0-beta.20",
    "prisma": "6.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "scroll-into-view-if-needed": "^3.1.0"
  },
  "devDependencies": {
    "@clack/prompts": "^0.7.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/compat": "^1.2.6",
    "@eslint/config-inspector": "^0.5.6",
    "@eslint/js": "^9.20.0",
    "@mermaid-js/mermaid-cli": "^11.4.2",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@next/eslint-plugin-next": "^14.2.24",
    "@octokit/request-error": "^6.1.7",
    "@octokit/rest": "^21.1.0",
    "@prisma/client": "5.19.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.17.18",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/typescript-estree": "^8.24.0",
    "@yamada-ui/calendar": "^1.8.14",
    "@yamada-ui/lucide": "^1.10.3",
    "@yamada-ui/markdown": "^1.1.1",
    "@yamada-ui/table": "^1.3.11",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.20.1",
    "eslint-config-next": "14.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
